<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets>
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Insert with Set Clause</Title>
      <Shortcut>InsertWithSet</Shortcut>
      <Description>Insert the value into the column using set statement.</Description>
      <Author>Devart</Author>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>table_name</ID>
          <ToolTip>table_name</ToolTip>
          <Default>TABLE1</Default>
        </Literal>
        <Literal>
          <ID>column1_name</ID>
          <ToolTip>column1_name</ToolTip>
          <Default>COLUMN1</Default>
        </Literal>
        <Literal>
          <ID>value_column1</ID>
          <ToolTip>value_column1</ToolTip>
          <Default>'String1'</Default>
        </Literal>
      </Declarations>
      <Code Language="SQL (Devart)" Kind="MySql"><![CDATA[INSERT DELAYED INTO $table_name$
  SET $column1_name$ = $value_column1$;
$caret$]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>