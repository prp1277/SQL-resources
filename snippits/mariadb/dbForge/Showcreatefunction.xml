    <Header>
      <Title>Show Create Function</Title>
      <Shortcut>ShowCreateFunction</Shortcut>
      <Description>Show DDL of function.</Description>
      <Author>Devart</Author>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>FunctionName</ID>
          <ToolTip>FunctionName</ToolTip>
          <Default>FunctionName</Default>
        </Literal>
      </Declarations>
      <Code Language="SQL (Devart)" Kind="MySql"><![CDATA[SHOW CREATE FUNCTION $FunctionName$;
$caret$]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>