<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets>
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Create Procedure with parameters</Title>
      <Shortcut>CreateProcedure</Shortcut>
      <Description>Create procedure with parameters.</Description>
      <Author>Devart</Author>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>procedure_name</ID>
          <ToolTip>procedure_name</ToolTip>
          <Default>PROCEDURE1</Default>
        </Literal>
        <Literal>
          <ID>IN_parameter_name</ID>
          <ToolTip>IN_parameter_name</ToolTip>
          <Default>PARAM1</Default>
        </Literal>
        <Literal>
          <ID>IN_parameter_type</ID>
          <ToolTip>IN_parameter_type</ToolTip>
          <Default>VARCHAR(20)</Default>
        </Literal>
        <Literal>
          <ID>OUT_parameter_name</ID>
          <ToolTip>OUT_parameter_name</ToolTip>
          <Default>PARAM2</Default>
        </Literal>
        <Literal>
          <ID>OUT_parameter_type</ID>
          <ToolTip>OUT_parameter_type</ToolTip>
          <Default>INT</Default>
        </Literal>
      </Declarations>
      <Code Language="SQL (Devart)" Kind="MySql"><![CDATA[CREATE PROCEDURE $procedure_name$(
 IN $IN_parameter_name$ $IN_parameter_type$,
 OUT $OUT_parameter_name$  $OUT_parameter_type$)
CONTAINS SQL
BEGIN
$caret$
END;
]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>