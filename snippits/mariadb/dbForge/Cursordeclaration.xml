<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets>
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Declare Cursor</Title>
      <Shortcut>Cursor</Shortcut>
      <Description>Cursor declaration snippet.</Description>
      <Author>Devart</Author>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>cursor_name</ID>
          <ToolTip>cursor_name</ToolTip>
          <Default>CURSOR1</Default>
        </Literal>
        <Literal>
          <ID>table_owner</ID>
          <ToolTip>table_owner</ToolTip>
          <Default>TEST</Default>
        </Literal>
        <Literal>
          <ID>table_name</ID>
          <ToolTip>table_name</ToolTip>
          <Default>TABLE1</Default>
        </Literal>
        <Literal>
          <ID>column1_name</ID>
          <ToolTip>column1_name</ToolTip>
          <Default>COLUMN1</Default>
        </Literal>
      </Declarations>
      <Code Language="SQL (Devart)" Kind="MySql"><![CDATA[DECLARE $cursor_name$ CURSOR FOR
  SELECT $table_name$.$column1_name$ 
FROM
  $table_owner$.$table_name$;$caret$]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>