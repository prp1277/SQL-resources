      <Declarations>
        <Literal>
          <ID>trigger_name</ID>
          <ToolTip>trigger_name</ToolTip>
          <Default>TRIGGER1</Default>
        </Literal>
        <Literal>
          <ID>owner_name</ID>
          <ToolTip>owner_name</ToolTip>
