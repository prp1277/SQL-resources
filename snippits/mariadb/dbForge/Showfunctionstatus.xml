<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets>
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Show Function Status</Title>
      <Shortcut>ShowFunctionStatus</Shortcut>
      <Description>Show function status.</Description>
      <Author>Devart</Author>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>function_name</ID>
          <ToolTip>function_name</ToolTip>
          <Default>FUNCTION1</Default>
        </Literal>
      </Declarations>
      <Code Language="SQL (Devart)" Kind="MySql"><![CDATA[SHOW FUNCTION STATUS LIKE '$function_name$';
$caret$]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>